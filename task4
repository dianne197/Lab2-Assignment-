 */
public class Lab2Task4 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int tempF = 212;
        int celsius = (tempF - 32) * 5/9;
    System.out.println("212 fahrenheit is equal to " + celsius +" celsius");
    }
    
}
